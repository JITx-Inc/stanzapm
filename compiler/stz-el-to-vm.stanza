      ;  defn call-imm (f:EImm) :
      ;    within l = store-into-xs(x(ins)) :
      ;      emit(CallIns(l, imm(f), ys*))
      ;  match(/f(ins)) :
      ;    (f:EVar) :
      ;      if function?(global-table, n(f)) : call-global(n(f))
      ;      else : call-imm(f)
      ;    (f) : call-imm(f)
      ;(ins:EDump) :
      ;  val ys* = for y in imms(ys(ins)) map :
      ;    match(y:Local) :
      ;      y
      ;    else :
      ;      val x = makedef(type(y))
      ;      emit(SetIns(x,y))
      ;      x
      ;  emit(DumpIns(ys*))
      ;(ins:EReturn) :
      ;  emit(ReturnIns([imm(y(ins))]))
      (ins:EDef) :
        match(y(ins)) :
          ;Initialize local to given value
          (y:EImm) :
            put(x(ins), cval(y))
          ;Initialize local to fresh value
          (y:False) :
            val xs = imms(immtable[n(x(ins))]) as Tuple<Local>
            for x in xs do :
              val t = type(defs[n(x)])
              val v = match(t) :
                (t:VMByte) : NumConst(0Y)
                (t:VMInt) : NumConst(0)
                (t:VMLong) : NumConst(0L)
                (t:VMFloat) : NumConst(0.0f)
                (t:VMDouble) : NumConst(0.0)
                (t:VMRef) : Marker(VM-VOID-TYPE-ID)
              emit(SetIns(x, v))
      (ins:EInitClosures) :
        ???

