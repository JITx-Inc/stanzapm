defpackage stz/timing :
  import core
  import collections

var TIMER-START:Long = 0L
public defn start-timer (name:String) :
  println("%_: Starting Timer" % [name])
  TIMER-START = current-time-ms()
public defn tick (name:String) :
  val time-passed = current-time-ms() - TIMER-START
  println("%_: %_ ms since start." % [name, time-passed])
public lostanza defn tick (name:ptr<byte>) -> ref<False> :
  tick(String(name))
  return false